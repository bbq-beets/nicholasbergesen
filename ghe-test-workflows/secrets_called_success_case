name: called
on:
  workflow_call:
    secrets:
      reposecret:
        required: true
      envsecret:
        required: true
      orgsecret:
        required: true
jobs:
  build:
    runs-on: [ self-hosted ]
    environment: prod
    steps:
      - name: Repo
        run: echo ${{ secrets.reposecret }}
      - name: Env
        run: echo ${{ secrets.envsecret }}
      - name: Org
        run: echo ${{ secrets.orgsecret }}
